<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m">
  <App>
    <Table id="employeeTable" items="{myOdata999>/Employees}" growing="true">
      <columns>
        <Column>
          <Label text="Employee ID" />
        </Column>
        <Column>
          <Label text="Name" />
        </Column>
        <Column>
          <Label text="City" />
        </Column>
        <Column>
          <Label text="Notes" />
        </Column>
      </columns>
      <items>
        <ColumnListItem>
          <Text text="{myOdata999>EmployeeID}" />
          <Text text="{parts:[{path:'myOdata999>FirstName'},{path:'myOdata999>LastName'}]}" />
          <Text text="{myOdata999>City}" />
          <Text text="{myOdata999>Notes}" />
        </ColumnListItem>
      </items>
    </Table>
  </App>
</mvc:View>