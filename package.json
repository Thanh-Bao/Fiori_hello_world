{
	"name": "baobaostore.com",
	"version": "0.3.0",
	"description": "baobaostore.com build with OpenUI5",
	"private": true,
	"engines": {
		"node": "^16.18.0 || >=18.12.0",
		"npm": ">= 8"
	},
	"scripts": {
		"start": "ui5 serve --open index.html",
		"lint": "eslint webapp",
		"build": "ui5 build -a --clean-dest",
		"build-self-contained": "ui5 build self-contained -a --clean-dest",
		"serve-dist": "ws --compress -d dist"
	},
	"dependencies": {
		"@openui5/sap.f": "^1",
		"@openui5/sap.m": "^1",
		"@openui5/sap.ui.core": "^1",
		"@openui5/sap.ui.layout": "^1",
		"@openui5/themelib_sap_belize": "^1",
		"@openui5/themelib_sap_horizon": "1.112.0"
	},
	"devDependencies": {
		"@openui5/ts-types-esm": "^1.112.0",
		"@sap/eslint-plugin-ui5-jsdocs": "^2.0.8",
		"@sapui5/ts-types-esm": "^1.108.12",
		"@types/jquery": "^3.5.16",
		"@ui5/cli": "^3.0.6",
		"eslint": "^8.37.0",
		"local-web-server": "^5.3.0",
		"rimraf": "^4.4.1",
		"ui5-middleware-livereload": "^0.8.2"
	},
	"ui5": {
		"dependencies": [
			"ui5-middleware-livereload"
		]
	}
